    "provide": {
        "ext-mongo": "1.6.14"
    },
    "suggest": {
        "dflydev/psr0-resource-locator-service-provider": "1.0.*@dev"
    },
    "autoload": {
        "psr-4": { "Saxulum\\DoctrineMongoDbOdm\\": "src/" }
    }
