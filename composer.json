{
    "name": "saxulum/saxulum-doctrine-mongodb-odm-provider",
    "description": "Saxulum Doctrine MongoDB ODM Provider",
    "keywords": ["saxulum", "pimple", "cilex", "silex", "doctrine", "mongo", "mongodb", "odm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "~7.0|~5.6",
        "alcaeus/mongo-php-adapter": "^1.1.3",
        "doctrine/mongodb-odm": "~1.3",
        "pimple/pimple": ">=2.1,<4",
        "saxulum/saxulum-doctrine-mongodb-provider": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
